{
  "name": "rnstarter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "production": "yarn rnuc production.env",
    "staging": "yarn rnuc staging.env",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "resetIos": "yarn staging && cd ios && pod deintegrate && rm -rf Podfile.lock && pod install && cd .. && yarn run ios",
    "resetAndroid": "yarn staging && cd android && ./gradlew clean && cd .. && yarn run android",
    "buildApkStaging": "yarn staging && cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "buildApkProduction": "yarn production && cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "pushAndroidStaging": "yarn staging && appcenter codepush release-react -a Mobilecoderz-ReactNative/RNStarter-Android -d Staging",
    "pushIosStaging": "yarn staging && appcenter codepush release-react -a Mobilecoderz-ReactNative/RNStarter-iOS -d Staging",
    "pushStaging": "yarn pushAndroidStaging && yarn pushIosStaging",
    "pushAndroidProduction": "yarn production && appcenter codepush release-react -a Mobilecoderz-ReactNative/RNStarter-Android -d Production",
    "pushIosProduction": "yarn production && appcenter codepush release-react -a Mobilecoderz-ReactNative/RNStarter-iOS -d Production",
    "pushProduction": "yarn pushAndroidProduction && yarn pushIosProduction",
    "postinstall": "yarn staging && npx pod-install"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^5.0.1",
    "@react-native-community/netinfo": "^7.1.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/moment": "^2.13.0",
    "@types/react-native-background-timer": "^2.0.0",
    "@types/react-native-indicators": "^0.16.1",
    "@types/react-native-material-menu": "^1.0.6",
    "@types/react-native-vector-icons": "^6.4.10",
    "appcenter": "^4.1.0",
    "appcenter-analytics": "^4.1.0",
    "appcenter-crashes": "^4.1.0",
    "axios": "^0.24.0",
    "fs": "^0.0.1-security",
    "google-libphonenumber": "^3.2.25",
    "moment": "^2.29.1",
    "moti": "^0.17.1",
    "react": "17.0.2",
    "react-hook-form": "^7.22.1",
    "react-native": "0.66.4",
    "react-native-background-timer": "^2.4.1",
    "react-native-bootsplash": "^3.2.3",
    "react-native-code-push": "^7.0.4",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-dropdownalert": "^4.3.0",
    "react-native-gesture-handler": "1.10.3",
    "react-native-image-crop-picker": "^0.37.3",
    "react-native-indicators": "^0.17.0",
    "react-native-internet-connection-alert": "^0.1.6",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-localize": "^2.1.6",
    "react-native-material-menu": "^1.2.0",
    "react-native-mmkv-storage": "^0.6.6",
    "react-native-modal": "^13.0.1",
    "react-native-modal-datetime-picker": "^13.0.0",
    "react-native-permissions": "^3.3.0",
    "react-native-pixel-perfect": "^1.0.2",
    "react-native-reanimated": "^2.3.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
